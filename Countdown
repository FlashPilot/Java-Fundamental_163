package hello;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner in = new Scanner(System.in);
		int count = 100;
		do
		{
			count = count-1;
			System.out.println(count);
		}while (count >= 0);
		System.out.println(count);
        System.out.println("发射！");
	}
}
