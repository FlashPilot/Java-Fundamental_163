package hello;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
        int BJT;
        int UTC = 0;
        
        Scanner in = new Scanner(System.in);
        BJT = in.nextInt();
        
        if (BJT >= 800)
        {
        	UTC = BJT - 800;
        }
        else 
        {
        	UTC = BJT +1600;
        }
        
        System.out.println(UTC);
	}

}




