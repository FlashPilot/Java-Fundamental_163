package hello;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int sign = 1;
		double sum = 0.0;
		
		for (int i = 1; i <= n; i++,sign = -sign)
		{
			sum += sign*1.0/i;
			
			
//			if (i%2 == 1)
//			{
//				sum += 1.0/i;
//			}
//			else
//			{
//				sum -= 1.0/i;
//			}
		}
		
		System.out.printf("%.2f",sum);
	}
}
