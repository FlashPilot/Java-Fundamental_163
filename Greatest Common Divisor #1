package hello;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int a = in.nextInt();
		int b = in.nextInt();
		int gcd = 1;
		
		for ( int i = 2; i<=a && i<=b;i++)
		{
			if ( a % i == 0 && b % i == 0)
			{
				gcd = i;
			}
		}
		
		System.out.printf("The greatest common divisor of a and b is "+gcd+"." );
	}
}
